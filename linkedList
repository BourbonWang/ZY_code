#include<stdio.h>
#include<stdlib.h>
#pragma warning (disable:4996)
struct student {
	int a;
	struct student *next;
};
#define Len sizeof(struct student)
struct student* creat()
{
	struct student *head, *p1, *p2;
	p1=p2 = (struct student*)malloc(Len);
	scanf("%d", &p1->a);
	int n = 0;
	for (head = NULL; p1->a != 0;n++)
	{
		if (n == 0) head = p1;
		else p2->next = p1;
		p2 = p1;
		p1 = (struct student*)malloc(Len);
		scanf("%d", &p1->a);
	}
	p2->next = NULL;
	return head;
}

void output(struct student* head)
{
	struct student *p;
	for (p = head; p != NULL; p = p->next)
		printf("%d,", p->a);
	return;
}

struct student* del(struct student *head)
{
	struct student *p, *q;
	for (p = head->next, q = head; p != NULL;p=p->next)
		if (q->a == p->a) q->next = p->next;
		else q = q->next;
	return head;
}

struct student* change(struct student *head)
{
	struct student *p, *q, *head1, *pmax, *qmax;
	head1 = NULL;
	while (head!= NULL)
	{
		for ( q=pmax= head,qmax=NULL,p=head->next;p!=NULL;q=p, p = p->next)
			if (pmax->a < p->a) pmax = p, qmax = q;
		if (pmax == head) head = head->next;
		else qmax->next = pmax->next;
			pmax->next = head1;
			head1 = pmax;
	}
	return head1;
}


int main()
{
	struct student* creat();
	void output(struct student* head);
	struct student* del(struct student *head);
	struct student* change(struct student *head);
	struct student *head;
	head =creat();
	head = change(head);
	head = del(head);
	output(head);
	system("pause");
	return 0;
}
